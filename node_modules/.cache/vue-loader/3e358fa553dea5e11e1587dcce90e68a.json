       <img :src=\"pet.pet_photo\" :alt=\"pet.pet_name\">\n                <!-- / project-image -->\n\n                <!-- project-hover-tools -->\n                <div class=\"project-hover-tools\">\n                    <router-link v-bind:to=\"'/pets/' + pet.pet_id\" class=\"view-btn\">\n                        <i class=\"lnr lnr-eye\"></i>\n                    </router-link>\n                </div><!-- / project-hover-tools -->\n\n                <!-- project-details -->\n                <div class=\"project-details\">\n                    <h4>{{ pet.pet_name }}</h4>\n                    <h5>{{ pet.pet_age }}</h5>\n                      <h5 v-if=\"Array.isArray(pet.pet_breeds)\">\n                        {{pet.pet_breeds[0][\"$t\"]}}\n                      </h5>\n                      <h5 v-else> \n                        {{pet.pet_breeds.$t }} \n                      </h5>\n                </div><!-- / project-details -->\n            </div><!-- / img-bg-color -->\n        </li>\n      </ul>\n    </section>\n\n    <!-- / portfolio section -->\n\n    <!-- / content -->\n\n  </div>\n</template>\n\n\n<style>\n  /*div.img-bg-color.primary {\n    height: 400px;\n  }*/\n  div img {\n    height: 100%;\n    width: auto;\n  }\n  div.project-details h4, h5 {\n    color: #e0e1e2;\n  }\n\n  /*div.project-hover-tools {\n    margin: 0, 50px, 0, 0;\n  }*/\n</style>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data: function() {\n    return {\n      pets: [],\n      user: {},\n    };\n  },\n  created: function() {\n    axios.get(\"/api/pets\").then(response => {\n      this.pets = response.data;\n      console.log(response.data);\n    });\n    axios.get(\"/api/users/me\").then(response => {\n      this.user = response.data;\n      console.log(response.data);\n    });\n  },\n  methods: {}\n};\n\n</script>\n"]}]}