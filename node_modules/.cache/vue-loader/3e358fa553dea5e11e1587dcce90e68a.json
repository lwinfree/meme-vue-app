iqBreeds\">{{pet.breed_list}}</option>\n      </datalist>\n\n      <div>\n        <button type=\"button\" class=\"btn btn-sm btn-primary-filled\" v-on:click=\"setSortAttribute('pet_age')\">Sort by Age\n          <i v-if=\"sortAttribute == 'pet_age' && sortAscending == 1\">^</i>\n          <i v-if=\"sortAttribute == 'pet_age' && sortAscending == -1\">v</i>\n        </button>\n      </div>\n\n\n      <ul class=\"row portfolio list-unstyled lightbox\" id=\"grid\">\n        <li class=\"col-xs-6 project\" v-for=\"pet in orderBy(filterBy(pets, breedFilter, 'breed_list'), sortAttribute, sortAscending)\" v-bind:key=\"pet.pet_id\">\n            <div class=\"img-bg-color primary\">\n                <!-- / project-link -->\n                <img :src=\"pet.pet_photo\" :alt=\"pet.pet_name\">\n                <!-- / project-image -->\n\n                <!-- project-hover-tools -->\n                <div class=\"project-hover-tools\">\n                    <router-link v-bind:to=\"'/pets/' + pet.pet_id\" class=\"view-btn\">\n                        <i class=\"lnr lnr-eye\"></i>\n                    </router-link>\n                </div><!-- / project-hover-tools -->\n\n                <!-- project-details -->\n                <div class=\"project-details\">\n                    <h4>{{ pet.pet_name }}</h4>\n                    <h5>{{ pet.pet_age }}</h5>\n                      <h5 v-if=\"Array.isArray(pet.pet_breeds)\">\n                        {{pet.pet_breeds[0][\"$t\"]}}\n                      </h5>\n                      <h5 v-else> \n                        {{pet.pet_breeds.$t }} \n                      </h5>\n                </div><!-- / project-details -->\n            </div><!-- / img-bg-color -->\n        </li>\n      </ul>\n    </section>\n\n    <!-- / portfolio section -->\n\n    <!-- / content -->\n\n  </div>\n</template>\n\n\n<style>\n  div.img-bg-color.primary {\n    height: 400px;\n  }\n  div img {\n    height: 100%;\n    width: auto;\n  }\n  div.project-details h4, h5 {\n    color: #e0e1e2;\n  }\n\n</style>\n\n<script>\nimport axios from \"axios\";\nimport Vue2Filters from \"vue2-filters\";\nimport uniq from 'lodash/uniq';\nimport underscore from 'vue-underscore';\n\nexport default {\n  mixins: [Vue2Filters.mixin],\n  data: function() {\n    return {\n      pets: [],\n      user: {},\n      breedFilter: '',\n      sortAttribute: 'breed_list',\n      sortAscending: 1,\n      breedList: ''\n    };\n  },\n  computed:{\n    uniqBreeds () {\n      return _uniqBy(this.pets, 'breed_list')\n    }\n  },\n\n  created: function() {\n    axios.get(\"/api/pets\").then(response => {\n      this.pets = response.data;\n      console.log(response.data);\n    });\n    axios.get(\"/api/users/me\").then(response => {\n      this.user = response.data;\n      console.log(response.data);\n    });\n  },\n  methods: {\n    setSortAttribute: function(attribute) {\n      if (this.sortAttribute === attribute) {\n        this.sortAscending = this.sortAscending * -1;\n      } else {\n        this.sortAscending = 1;\n      }\n      this.sortAttribute = attribute;\n    }\n  }\n};\n\n</script>\n\n<!-- filterBy(pets, 'Baby', 'pet_age') -->"]}]}