\n    };\n  },\n  methods: {\n    submit: function() {\n      var params = {\n        name: this.name,\n        email: this.email,\n        zipcode: this.zipcode,\n        password: this.password,\n        password_confirmation: this.passwordConfirmation\n      };\n      axios\n        .post(\"/api/users/\", params)\n        .then(response => {\n          this.$router.push(\"login\");\n        })\n        .catch(error => {\n          this.errors = error.response.data.errors;\n        });\n    }\n  }\n};\n</script>"]}]}