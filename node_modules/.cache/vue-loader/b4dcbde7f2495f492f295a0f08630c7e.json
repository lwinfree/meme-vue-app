log(error.response.data);\n      });\n    },\n    unfavoritePet: function() {\n      axios.delete(\"/api/favorites/\" + this.pet.pet_id).then(response => {\n        console.log(response.data);\n        this.pet.favorite = false;\n      }).catch(error => {\n        console.log(error.response.data);\n      });\n    }\n  }\n};\n\n</script>"]}]}